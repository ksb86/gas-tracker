{
  "name": "gas-tracker",
  "version": "1.0.4",
  "description": "Code for gas tracker (react, heroku, etc)",
  "main": "index.js",
  "scripts": {
    "prod": "NODE_ENV=prod concurrently 'yarn' 'yarn clean' && yarn build:prod && yarn restart",
    "restart": "pm2 stop gas-tracker; pm2 start --name gas-tracker ./server.js",
    "dev": "NODE_ENV=dev concurrently -n 'build:dev,server' -c 'bgBlue.bold,bgGreen.bold' 'yarn build:dev' 'yarn server'",
    "build:dev": "webpack --config webpack.dev.js -w",
    "build:prod": "webpack --config webpack.prod.js -p",
    "server": "nodemon ./server",
    "clean": "echo hi",
    "devOld": "concurrently 'webpack-dev-server --open --content-base dev/' 'webpack --watch'",
    "prodOld": "yarn && webpack --config webpack.build.config.js -p && yarn restart",
    "restartOld": "pm2 stop gas-tracker; pm2 start --name gas-tracker ./bin/www",
    "buildOld": "webpack --config webpack.build.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksb86/gas-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-preset-es2015-node-auto": "^0.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "firebase": "^4.1.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "morgan": "^1.8.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-mixin": "^3.0.5",
    "reactfire": "^1.0.0",
    "serve-favicon": "^2.4.3",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.3",
    "nodemon": "^1.18.11",
    "pm2": "^3.2.2"
  }
}
